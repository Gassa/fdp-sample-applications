{
  "labels": {
    "application": "model-server-akkastreams"
  },
  "id": "/apps/model-server-akkastreams",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "container": {
    "portMappings": [],
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "fdp-reg.lightbend.com:443/model-server-akkastreams:1.1.0",
      "forcePullImage": true,
      "privileged": false,
      "parameters": []
    }
  },
  "cpus": 1,
  "disk": 100,
  "env": {
    "KAFKA_BROKERS_LIST": "broker.kafka.l4lb.thisdcos.directory:9092",
    "ZOOKEEPER_URL": "master.mesos:2181/dcos-service-kafka",
    "GRAFANA_HOST": "grafana.marathon.l4lb.thisdcos.directory",
    "GRAFANA_PORT": "3000",
    "INFLUXDB_HOST": "influxdb.marathon.l4lb.thisdcos.directory",
    "INFLUXDB_PORT": "8086"
  },
  "instances": 1,
  "maxLaunchDelaySeconds": 3600,
  "mem": 256,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "healthChecks": [],
  "fetch": [],
  "constraints": []
}